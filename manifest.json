{
    "name": "FaviconTab",
    "version": "0.1",
    "manifest_version": 2,

    "description": "FaviconTab",

    "options_page": "html/options.html",
    "permissions": [
        "bookmarks", "tabs", "file://*", "chrome://favicon/"
    ],
    "chrome_url_overrides": {
        "newtab": "html/favicontab.html"
    },
    "background" : {
        "scripts": [
          "js/installed.js"
        ],
        "persistent": false
    }
}

